{% if nagios_plugins is defined %}
{% for i in nagios_plugins %}
command[{{ i.command }}]={% if require_sudo in i and i.require_sudo %}sudo {% endif %}{{ i.path }} {{ i.arguments }}
{% endfor %}
{% endif %}

{%- if nrpe_checks is defined %}
{% for i in nrpe_checks %}
command[{{ i.command }}]={{ i.path }} {{ i.arguments }}
{% endfor %}
{% endif %}
